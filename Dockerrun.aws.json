{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "drm_backend_web",
      "image": "sameeranbandishti/drm_backend_web",
      "hostname": "web",
      "memory": 256,
      "portMappings": [
        {
          "containerPort": 8000
        }
      ],
      "mountPoints": [
        {
          "sourceVolume": "static_data",
          "containerPath": "/vol/web"
        }
      ]
    },
    {
      "name": "drm_backend_nginx",
      "image": "sameeranbandishti/drm_backend_nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "links": [
        "drm_backend_web"
      ],
      "memory": 128,
      "mountPoints": [
        {
          "sourceVolume": "static_data",
          "containerPath": "/vol/web"
        }
      ]
    }
  ],
  "volumes": {
    "name": "static_data",
    "host": {}
  }
}