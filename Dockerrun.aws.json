{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "drm_backend_web",
      "image": "sameeranbandishti/drm_backend_web",
      "hostname": "web",
      "memory": 256
    },
    {
      "name": "drm_backend_nginx",
      "image": "sameeranbandishti/drm_backend_nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "links": [
        "drm_backend_web"
      ],
      "memory": 128
    }
  ]
}